<body>
    <div class="header">
        <!-- <h3 class="logo"></h3> -->
        <img src="/assets/img/SkillerBot_Logo.png" alt="Skiller Bot" class="logo" />
        <div class="hamburger" id="menuButton" onmouseover="shrinkMenu()" onmouseout="expandMenu()" onclick="openMenu();">
            <span id="line1"></span>
            <span id="line2" class=""></span>
            <span id="line3"></span>
        </div>
    </div>

    <form class="questionnaireSection" action="#" onsubmit="return saveUserData()" method="get" >
        <div class="userInputLabel"> <% questions.forEach(function(question){%>
            <%=question %><br></div>
            <input type="text" class="userInputDetails" placeholder="Your Name ..." required><br>
        <% }); %>
    </form>

